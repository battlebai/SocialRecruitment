
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>


    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/main.css">
    <link href="~/Content/pagerstyles.css" rel="stylesheet" />

    <script type="text/javascript" src="~/Scripts/jquery.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validate.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/My97DatePicker/WdatePicker.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/JScriptForm.js"></script>
    <script src="~/Scripts/Validate.js" type="text/javascript"></script>
    <script src="~/Scripts/JScriptCommon.js" type="text/javascript"></script>
    <link rel="Stylesheet" href="~/Content/Competence.css" />
    <script type="text/javascript" src="~/Scripts/jquery.autocomplete.js"></script>

    @RenderSection("HeadContent", false)

</head>
<body>
    <div>

        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <!-- logo -->
                    <a href="http://www.langben.com/">
                        <img class="logo" src="~/images/logo.png" alt="" style="width: 207px;" />
                    </a>
                </div>
                <div class="col-md-4">
                    <h3  style="float:right;" > 
                        <a href="~/SysNotice"  style="margin-right:20px;"  id="aTopNotice">我的消息</a>
                        <a href="~/Preview" id="aTopPersonalInformation">个人资料</a>
                    </h3>
                </div>
            </div>
        </div>

        <div class="container" style="min-height: 720px">
            <div class="row clearfix">
                @RenderBody()
                @RenderSection("SubMenu")
            </div>
        </div>
        <hr />
        <footer>
            <div class="footer">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <span class="footer-company"><a target="_blank" title="北京狼奔科技有限公司" href="http://www.langben.com/">北京狼奔科技有限公司</a></span>
                        </div>
                        <div class="col-md-6">
                            <div class="footer-right">
                                <span class="footer-company">邮箱</span> ben@langben.com
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    @Html.ValidationSummary(true)
</body>
</html>

